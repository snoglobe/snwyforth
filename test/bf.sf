0 6 trap dup
while : 
    dup if 91 = : ( l bracket )
        1 trap
    ;
    dup if 93 = : ( r bracket )
        1 trap
    ;
    dup if 43 = : ( plus )
        1 trap
    ;
    dup if 45 = : ( minus )
        1 trap
    ;
    dup if 44 = : ( comma )
        1 trap
    ;
    dup if 46 = : ( period )
        1 trap
    ;
    drop
;
